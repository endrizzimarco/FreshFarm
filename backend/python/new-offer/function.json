{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "type": "cosmosDBTrigger",
      "name": "new_offers",
      "direction": "in",
      "databaseName": "FreshFarmDB",
      "collectionName": "Offers",
      "feedPollDelay": 1000,
      "connectionStringSetting": "CosmosDbConnectionString"
    },
    {
      "type": "signalR",
      "name": "signalRMessage",
      "hubName": "freshfarm",
      "direction": "out",
      "connectionStringSetting": "AzureSignalRConnectionString"
    }
  ]
}