{
  "name": "freshfarm",
  "version": "0.0.1",
  "description": "An app to enable farmers to offer produce directly to consumers",
  "productName": "FreshFarm",
  "author": "Marco Endrizzi <marco.endrizzi.bz@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.34.0",
    "@quasar/extras": "^1.0.0",
    "axios": "^1.2.1",
    "chart.js": "^4.2.1",
    "mapbox-gl": "^2.13.0",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "three": "0.127.0",
    "troisjs": "0.2.3",
    "vue": "^3.0.0",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.16",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.2.1",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.0.3",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "tailwindcss": "^3.3.1",
    "vitest": "^0.29.1",
    "vue-pug-plugin": "^2.0.3",
    "webcrypto-shim": "^0.1.7",
    "jest": {
      "testEnvironment": "jsdom",
      "testEnvironmentOptions": {
        "resources": "usable",
        "runScripts": "dangerously",
        "crypto": {
          "hashes": "sha256"
        }
      }
    }
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
